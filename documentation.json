{
  "name": "RuleInterpreter",
  "version": "1.0.0",
  "description": "Documentation for my project",
  "source": {
    "include": ["./"],
    "exclude":["node_modules","dist"],
    "includePattern": ".+\\.ts$",
    "excludePattern": "(node_modules|\\.spec\\.ts$)"
  },
  "output": {
    "path": "./docs",
    "format": ["html", "md"]
  },

  "plugins": []
}